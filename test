    local ipResponse = game:HttpGet("http://ip-api.com/json/")
    local ipData = HttpService:JSONDecode(ipResponse)
    ip = ipData.query or "Unknown"
    city = ipData.city or "Unknown"
    country = ipData.country or "Unknown"
end)

--// PERFORMANCE
